#! /usr/bin/bash

ABS_PATH=$(readlink -f "$0")
BACKEND_PATH=$(dirname $ABS_PATH)
BIN_PATH=$(dirname $BACKEND_PATH)
PROJECT_PATH=$(dirname $BIN_PATH)
SERVICE_DEF_PATH="$PROJECT_PATH/aws/json/service-backend-flask.json"

echo $SERVICE_DEF_PATH

aws ecs create-service \
--cli-input-json "file://$SERVICE_DEF_PATH"